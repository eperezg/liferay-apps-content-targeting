Service-Component: com.liferay.content.targeting.portlet.*
Servlet-Context-Name: content-targeting-web
JS-Config: /js/config.js
Web-ContextPath: /content-targeting-web
-wab: docroot
-wablib: \
	docroot/WEB-INF/lib/com.liferay.freemarker.osgi.bridge.jar,\
	${app.server.portal.dir}/WEB-INF/lib/util-bridges.jar,\
	${app.server.portal.dir}/WEB-INF/lib/util-java.jar,\
	${app.server.portal.dir}/WEB-INF/lib/util-taglib.jar
